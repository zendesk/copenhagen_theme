          </div>
        {{/each}}
        <div class="subsections-list manual-article section-title-collection
          {{#each (slice (filter category.sections on="description" equals="Manual-Type") 0 1)}} mt-xl  mb-3xl{{/each}}">
          {{#each category.sections}}
            {{#is description 'Manual-Type' }}
              <div class="subsections-container mt-2xl" is-manual-type-exist>
                <div class="font-bw-1 title-4 bold">{{name}}</div>
                <ul class="article-list" id="um-{{id}}">
                  {{#each (slice articles 0 article_count)}}
                      <li class="article-list-item">
                          <div class="article-item">
                              <a href="{{url}}" class="article-list-link">
                                  <span class="book-icon font-bw-2 mr-m"></span>
                                  {{title}}
                                  {{#if internal}}{{dc 'icon-internal'}}{{/if}}
                              </a>
                          </div>
                      </li>
                  {{/each}}
                </ul>
              </div>
            {{/is}}
          {{/each}}
        </div>
      </div>

      {{!-- Tips-Type --}}
